{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"types": ["@cloudflare/workers-types"]
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
